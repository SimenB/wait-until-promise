{
  "name": "wait-until-promise",
  "version": "0.0.1",
  "description": "A simple utilty to wait until condition is true, returning a promise",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf coverage/ index.js test-es5.js",
    "compile": "babel waitUntilPromise.js -o index.js",
    "compile-test": "babel test.js -o test-es5.js",
    "cover": "babel-istanbul cover -x test-es5.js _mocha test-es5",
    "lint": "standard",
    "precover": "npm run lint && npm run compile-test",
    "prelint": "npm run clean",
    "prepublish": "not-in-install && npm run compile || echo this is npm install",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SimenB/wait-until-promise.git"
  },
  "keywords": [
    "jasmine",
    "wait",
    "promise",
    "waitsfor"
  ],
  "author": "Simen Bekkhus <sbekkhus91@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SimenB/wait-until-promise/issues"
  },
  "homepage": "https://github.com/SimenB/wait-until-promise#readme",
  "devDependencies": {
    "babel-cli": "^6.6.4",
    "babel-istanbul": "^0.6.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.5.2",
    "babel-plugin-transform-es2015-block-scoping": "^6.6.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.0",
    "babel-plugin-transform-es2015-parameters": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.0",
    "codeclimate-test-reporter": "^0.3.1",
    "coveralls": "^2.11.8",
    "gulp-babel": "^6.1.2",
    "in-publish": "^2.0.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "standard": "^6.0.7"
  },
  "dependencies": {}
}
